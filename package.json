{
  "name": "@uirouter/react-hybrid",
  "version": "0.0.14",
  "scripts": {
    "clean": "shx rm -rf lib lib-esm _bundles",
    "compile": "npm run clean && tsc && tsc --module es6 --outDir lib-esm",
    "bundle": "rollup -c && rollup -c --environment MINIFY",
    "fixmaps:lib": "tweak_sourcemap_paths -a --include 'lib/**/*.js.map' 'lib-esm/**/*.js.map'",
    "fixmaps:bundle": "tweak_sourcemap_paths -a --include '_bundles/**/*.js.map'",
    "build": "run-s compile bundle fixmaps:*",
    "watch": "watch 'npm run build && yalc push' src",
    "prepublishOnly": "npm run build",
    "release": "release --deps @uirouter/core @uirouter/react @uirouter/angularjs"
  },
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "1.0.11",
    "@uirouter/core": "5.0.12",
    "@uirouter/react": "0.5.5"
  },
  "peerDependencies": {
    "angular": "^1.5.0",
    "react": "^15.0.0"
  },
  "devDependencies": {
    "@types/angular": "~1.6.1",
    "@types/jquery": "^3.2.17",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@uirouter/publish-scripts": "2.0.8",
    "angular": "~1.6.1",
    "awesome-typescript-loader": "^3.0.0",
    "conventional-changelog": "^1.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "readline-sync": "^1.4.7",
    "rollup": "^0.52.3",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-visualizer": "^0.3.1",
    "rxjs": "^5.0.0",
    "shx": "^0.2.2",
    "typescript": "2.6.2",
    "watch": "^1.0.2",
    "webpack": "^3.10.0"
  },
  "main": "_bundles/ui-router-react-hybrid.js",
  "module": "lib-esm/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/react-hybrid.git"
  }
}